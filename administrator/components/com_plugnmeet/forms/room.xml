<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Mynaparrot\Component\Plugnmeet\Administrator\Field">
    <fieldset name="basic">
        <field name="id" type="hidden" readonly="true" class="readonly" default="0"
               description="JGLOBAL_FIELD_ID_DESC"/>
        <field type="text"
               name="room_id"
               label="COM_PLUGNMEET_ROOM_ID"
               class="text_area"
               readonly="true"
               required="false"
               filter="STRING"
               message="COM_PLUGNMEET_ROOM_ID"
               autocomplete="off"/>
        <field type="category"
               name="cat"
               extension="com_plugnmeet.rooms"
               required="true"
               label="COM_PLUGNMEET_FORM_LBL_ROOM_CAT"
               description="COM_PLUGNMEET_FORM_DESC_ROOM_CAT"
        />
        <field type="text"
               name="room_title"
               label="COM_PLUGNMEET_ROOM_TITLE"
               size="10"
               maxlength="50"
               default=""
               class="text_area"
               readonly="false"
               disabled="false"
               required="true"
               filter="STRING"
               message="COM_PLUGNMEET_ROOM_TITLE"
               autocomplete="off"/>
        <field name="alias"
               type="text"
               class="inputbox"
               generator="title"
               required="false"
               label="JFIELD_ALIAS_LABEL"
               description="JFIELD_ALIAS_DESC"
               hint="COM_PLUGNMEET_FORM_LBL_ROOM_ALIAS"/>
        <field type="editor"
               name="description"
               label="COM_PLUGNMEET_ROOM_DES"
               default=""
               cols="5"
               rows="10"
               height="300px"
               buttons="no"
               editor="tinymce|codemirror|none"
               filter="raw"
               required="false"/>
        <field type="text"
               name="moderator_pass"
               label="COM_PLUGNMEET_ROOM_MODERATOR_PASS"
               size="10"
               maxlength="50"
               default=""
               class="text_area"
               readonly="false"
               disabled="false"
               required="true"
               filter="STRING"
               message="COM_PLUGNMEET_ROOM_MODERATOR_PASS"
               autocomplete="off"/>
        <field type="text"
               name="attendee_pass"
               label="COM_PLUGNMEET_ROOM_ATTENDEE_PASS"
               size="10"
               maxlength="50"
               default=""
               class="text_area"
               readonly="false"
               disabled="false"
               required="true"
               filter="STRING"
               message="COM_PLUGNMEET_ROOM_ATTENDEE_PASS"
               autocomplete="off"/>
